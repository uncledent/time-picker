{
  "name": "react-awesome-time-picker",
  "version": "1.0.1",
  "description": "React TimePicker",
  "keywords": [
    "react",
    "react-time-picker",
    "react-component",
    "timepicker",
    "time-picker",
    "ui component",
    "ui",
    "component"
  ],
  "files": [
    "lib",
    "es",
    "assets/*.css",
    "assets/*.less",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "lib/index",
  "module": "es/index",
  "homepage": "https://github.com/uncledent/time-picker",
  "author": "danny@delar.de",
  "repository": {
    "type": "git",
    "url": "git@github.com:uncledent/time-picker.git"
  },
  "bugs": {
    "url": "https://github.com/uncledent/time-picker/issues"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "2.x",
    "moment": "2.26.0",
    "raf": "^3.4.1",
    "rc-trigger": "4.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "enzyme": "^3.8.0",
    "enzyme-to-json": "^3.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "father": "^2.24.1",
    "np": "^6.0.0",
    "rc-util": "^4.11.0",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development father doc dev --storybook",
    "build": "father doc build --storybook",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "gh-pages": "npm run build && father doc deploy",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "lint": "eslint src/ examples/ --ext .tsx,.ts,.jsx,.js",
    "test": "father test",
    "coverage": "father test --coverage",
    "now-build": "npm run build",
    "watch": "watch 'npm run compile'"
  }
}
